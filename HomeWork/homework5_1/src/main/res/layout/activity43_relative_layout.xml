<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    style="@style/LayoutStyle2">

    <TextView
        style="@style/TextViewStyle6"
        android:id="@+id/android"
        android:text="@string/android"
        android:layout_alignParentTop="true"/>

    <ImageView
        style="@style/ImageViewStyle3"
        android:id="@+id/image1"
        android:layout_below="@id/android"
        android:layout_marginStart="30dp"/>

    <TextView
        style="@style/TextViewStyle7"
        android:id="@+id/posts"
        android:text="@string/posts"
        android:layout_toEndOf="@id/image1"
        android:layout_below="@id/android"/>

    <TextView
        style="@style/TextViewStyle7"
        android:id="@+id/followers"
        android:text="@string/followers"
        android:layout_toEndOf="@id/posts"
        android:layout_below="@id/android"/>

    <TextView
        style="@style/TextViewStyle7"
        android:id="@+id/following"
        android:text="@string/following"
        android:layout_toEndOf="@id/followers"
        android:layout_below="@id/android"/>

    <TextView
        style="@style/TextViewStyle8"
        android:id="@+id/username"
        android:text="@string/username"
        android:layout_below="@id/image1"/>

    <TextView
        style="@style/TextViewStyle9"
        android:id="@+id/address"
        android:text="@string/address_description"
        android:layout_below="@id/username"/>

    <ImageView
        style="@style/ImageViewStyle4"
        android:id="@+id/image2"
        android:layout_below="@id/address"
        android:layout_marginStart="30dp"/>

    <ImageView
        style="@style/ImageViewStyle4"
        android:id="@+id/image3"
        android:layout_toEndOf="@id/image2"
        android:layout_below="@id/address"
        android:layout_marginStart="12dp"/>

    <ImageView
        style="@style/ImageViewStyle4"
        android:id="@+id/image4"
        android:layout_toEndOf="@id/image3"
        android:layout_below="@id/address"
        android:layout_marginStart="12dp"/>

    <ImageView
        style="@style/ImageViewStyle4"
        android:id="@+id/image5"
        android:layout_toEndOf="@id/image4"
        android:layout_below="@id/address"
        android:layout_marginStart="12dp"/>

    <ImageView
        style="@style/ImageViewStyle4"
        android:id="@+id/image6"
        android:layout_toEndOf="@id/image5"
        android:layout_below="@id/address"
        android:layout_marginStart="12dp"/>

    <ImageView
        style="@style/ImageViewStyle4"
        android:id="@+id/image7"
        android:layout_toEndOf="@id/image6"
        android:layout_below="@id/address"
        android:layout_marginStart="12dp"/>

    <ImageView
        style="@style/ImageViewStyle5"
        android:id="@+id/image8"
        android:src="@drawable/directions"
        android:layout_below="@id/image2"
        android:layout_marginStart="60dp"/>

    <ImageView
        style="@style/ImageViewStyle5"
        android:id="@+id/image9"
        android:src="@drawable/call"
        android:layout_toEndOf="@id/image8"
        android:layout_below="@id/image2"
        android:layout_marginStart="45dp"/>

    <ImageView
        style="@style/ImageViewStyle5"
        android:id="@+id/image10"
        android:src="@drawable/save"
        android:layout_toEndOf="@id/image9"
        android:layout_below="@id/image2"
        android:layout_marginStart="20dp"/>

    <ImageView
        style="@style/ImageViewStyle5"
        android:id="@+id/image11"
        android:src="@drawable/website"
        android:layout_toEndOf="@id/image10"
        android:layout_below="@id/image2"
        android:layout_marginStart="32dp"/>

    <TextView
        style="@style/TextViewStyle10"
        android:id="@+id/directions"
        android:text="@string/directions"
        android:layout_below="@id/image8"
        android:paddingStart="30dp"/>

    <TextView
        style="@style/TextViewStyle10"
        android:id="@+id/call"
        android:text="@string/call"
        android:layout_toEndOf="@id/directions"
        android:layout_below="@id/image8"
        android:paddingStart="14dp"/>

    <TextView
        style="@style/TextViewStyle10"
        android:id="@+id/save"
        android:text="@string/save"
        android:layout_toEndOf="@id/call"
        android:layout_below="@id/image8"
        android:paddingStart="14dp"/>

    <TextView
        style="@style/TextViewStyle10"
        android:id="@+id/website"
        android:text="@string/website"
        android:layout_toEndOf="@id/save"
        android:layout_below="@id/image8"
        android:paddingStart="14dp"/>

    <View
        style="@style/LineStyle"
        android:id="@+id/line"
        android:layout_below="@id/directions"/>

    <ImageView
        style="@style/ImageViewStyle3"
        android:id="@+id/image12"
        android:layout_below="@id/line"
        android:layout_marginTop="25dp"
        android:layout_marginStart="30dp"/>

    <ImageView
        style="@style/ImageViewStyle3"
        android:id="@+id/image13"
        android:layout_toEndOf="@id/image12"
        android:layout_below="@id/line"
        android:layout_marginTop="25dp"
        android:layout_marginStart="15dp"/>

    <ImageView
        style="@style/ImageViewStyle3"
        android:id="@+id/image14"
        android:layout_toEndOf="@id/image13"
        android:layout_below="@id/line"
        android:layout_marginTop="25dp"
        android:layout_marginStart="15dp"/>

</RelativeLayout>